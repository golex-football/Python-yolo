# IPC endpoints
MOONALT_IPC_IN=ipc:///tmp/yolo_input.sock
MOONALT_BROADTRACK_OUT=ipc:///tmp/broadtrack_in.sock

# Capture (test)
MOONALT_TEST_VIDEO=/path/to/clip.mp4
MOONALT_CAPTURE_MAX_FPS=30
MOONALT_LOG_EVERY=10

# Viewer (debug MP4; leave empty in live)
MOONALT_VIEWER_MP4=
MOONALT_VIEWER_FPS=30

# YOLO knobs (stable defaults)
MOONALT_YOLO_IMGSZ=640
MOONALT_YOLO_EVERY_N=2
MOONALT_YOLO_CONF=0.45
MOONALT_YOLO_IOU=0.55
MOONALT_YOLO_MAXDET=60
MOONALT_YOLO_RETINA=0
MOONALT_YOLO_HALF=0      # keep 0 to avoid dtype fuse issues
MOONALT_YOLO_CLASSES=0 32
MOONALT_BT_SEND_RAW=1
